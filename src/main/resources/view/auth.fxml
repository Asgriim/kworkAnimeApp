<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AuthController">
   <children>
      <AnchorPane prefHeight="56.0" prefWidth="600.0" style="-fx-background-color: #1b1b1b;">
         <children>
            <Button fx:id="RegistrationButton" layoutX="437.0" layoutY="13.0" mnemonicParsing="false" onAction="#OpenRegistrationPage" style="-fx-background-color: #ec7c26; -fx-background-radius: 6;" text="Зарегистрироваться">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="backButton" layoutX="14.0" layoutY="13.0" mnemonicParsing="false" onAction="#back" style="-fx-background-color: #ec7c26; -fx-background-radius: 6;" text="&lt; Назад">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <TextField fx:id="LoginField" layoutX="225.0" layoutY="154.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Text layoutX="169.0" layoutY="173.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Логин:" />
      <Text layoutX="165.0" layoutY="233.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Пароль:" />
      <Button fx:id="AuthButton" layoutX="271.0" layoutY="282.0" mnemonicParsing="false" onAction="#Auth" style="-fx-background-color: #ec7c26; -fx-background-radius: 6;" text="Войти">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <PasswordField fx:id="PasswordField" layoutX="225.0" layoutY="216.0">
         <font>
            <Font size="14.0" />
         </font>
      </PasswordField>
   </children>
</AnchorPane>
